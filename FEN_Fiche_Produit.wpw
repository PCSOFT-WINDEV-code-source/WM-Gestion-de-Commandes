     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          // La référence du produit doit être saisie
          SI SAI_Reference ~= "" ALORS
          	Erreur(<§$0001§>)
          	EcranPremier(SAI_Reference)
          	RETOUR
          FIN
          // Le prix du produit doit être saisi
          SI SAI_PrixHT ~= "" ALORS
          	Erreur(<§$0003§>)
          	EcranPremier(SAI_PrixHT)
          	RETOUR
          FIN
          // Lecture des informations saisies
          EcranVersFichier()
          
          // S'il s'agit d'un nouvel enregistrement
          SI Produit..NouvelEnregistrement = Vrai ALORS
          	// On l'ajoute
          	HAjoute(Produit)
          SINON
          	// On le modifie
          	HModifie(Produit)
          FIN
          
          SI gbImageModifiee ALORS
          	// Sauvegarde l'image associée à ce produit
          	HAttacheMémo(Produit,Photo, IMG_Photo..Valeur,hMémoImg)	
          FIN
          
          MaFenêtre..ValeurRenvoyée = Vrai
          
          // Grise le bouton d'enregistrement
          OPT_Enregistrer..Etat = Grisé
          
          ferme()
         type : 32

       code_elements :
        type_code : 4
        p_codes :
         -
           code : |1+
            // Recherche la commande
            hlitrecherchepremier(commande, numcommande, ATT_NumCommande)
            si htrouve(commande) ALORS
            
            	// Création de la facture équivalente
            	HRAZ(Facture)
            	Facture.DateFacture = DateDuJour()
            	Facture.NumClient = Commande.NumClient
            	Facture.NumCommande = Commande.NumCommande
            	Facture.TotalHT = Commande.TotalHT
            	Facture.TotalTVA = Commande.TotalTVA
            	Facture.TotalTTC = Commande.TotalTTC
            	HAjoute(Facture)
            	
            	// Création des lignes de facture équivalentes
            	POUR TOUT LigneCde AVEC NumCommande = Commande.NumCommande
            		HLitRecherchePremier(Produit, Reference, LigneCde.Reference)
            		
            		HRAZ(LigneFac)
            		LigneFac.IDLigneCde = LigneCde.IDLigneCde
            		LigneFac.LibProd = Produit.LibProd
            		LigneFac.NumFacture = Facture.NumFacture
            		LigneFac.PrixVente = LigneCde.PrixVente
            		LigneFac.Quantite = LigneCde.Quantite
            		LigneFac.Reference = LigneCde.Reference
            		HAjoute(LigneFac)
            	FIN
            	
            	// Ajout d'un historique
            	AjouteHistorique(Commande.NumClient, cstTYPEHISTO_FACTURE, ChaîneConstruit(<§$0002§>, Facture.NumFacture))
            	
            	// Rafraîchit les informations de factures de la ligne
            	ZR_Commande[ZR_Commande].ATT_InfosFacture = ChaîneConstruit(<§$0005§>, DateVersChaîne(Facture.DateFacture, <§$0006§>))
            	ZR_Commande[ZR_Commande].ATT_FacturationPossible = faux	// Invisible
            	
            	// Message d'information
            	Info(<§$0004§>, ChaîneConstruit(<§$0003§>,Facture.NumFacture))
            	
            FIN

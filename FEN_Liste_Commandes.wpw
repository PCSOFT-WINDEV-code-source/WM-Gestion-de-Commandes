  procedures :
   -
     name : ListeCommande
     procedure_id : 1493208409106778252
     type_code : 14
     code : |1-
      // Résumé : Liste les commandes
      // Syntaxe :
      //ListeCommande ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ListeCommande()
      
      nIndice est un entier
      
      
      // Vide la zone répétée
      ZoneRépétéeSupprimeTout(ZR_Commande)
      
      // Parcourt les commandes
      pour tout commande sur DateCommande depuisfin
      	 
      	 // Ajoute une ligne
      	 nIndice = ZoneRépétéeAjouteLigne(ZR_Commande, commande.NumCommande)
      	 
      	// Informations sur le client
      	HLitRecherchePremier(Client, NumClient, Commande.NumClient)
      	SI HTrouve(Client) ALORS
      		
      		ZR_Commande[nIndice].ATT_InfosClient = Client.NomContact +[" "]+ Client.PrénomContact	+RC+ Client.Societe
      		
      		// Adresse de facturation
      		HLitRecherchePremier(Adresse, CLE_NumClientType, [Client.NumClient, "Facturation"])
      		SI HTrouve(Adresse) ALORS
      			ZR_Commande[nIndice].ATT_InfosClient += [RC] + Adresse.Adresse +[RC]+ Adresse.Adresse2 +[RC]+ Adresse.CodePostal +" "+ Adresse.Ville +[RC]+ Adresse.Pays
      		FIN
      		
      		// Adresse de livraison
      		HLitRecherchePremier(Adresse, CLE_NumClientType, [Client.NumClient, "Livraison"])
      		SI HTrouve(Adresse) ALORS
      			ZR_Commande[nIndice].ATT_AdresseLivraison += [RC] + Adresse.Adresse +[RC]+ Adresse.Adresse2 +[RC]+ Adresse.CodePostal +" "+ Adresse.Ville +[RC]+ Adresse.Pays
      		FIN
      		
      	FIN
      	
      	// Informations sur la commande
      	ZR_Commande[nIndice].ATT_DateCommande = commande.DateCommande
      	ZR_Commande[nIndice].ATT_NumCommandeEnClair = ChaîneConstruit(<§$0007§>, Commande.NumCommande)
      	ZR_Commande[nIndice].ATT_TotalTTC = ChaîneConstruit(<§$0008§>,  Commande.TotalTTC)
      	
      	// Informations sur la facture
      	HLitRecherchePremier(Facture, NumCommande, Commande.NumCommande)
      	SI HTrouve(Facture) ALORS
      		
      		// Commande déjà facturée
      		ZR_Commande[nIndice].ATT_InfosFacture = ChaîneConstruit(<§$0009§>, DateVersChaîne(Facture.DateFacture, maskDateSystème))
      		ZR_Commande[nIndice].ATT_FacturationPossible = faux
      		
      	SINON // Non, pas de facture
      		
